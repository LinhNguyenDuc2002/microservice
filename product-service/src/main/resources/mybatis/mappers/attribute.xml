<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.productservice.mybatis.mapper.AttributeMapper">
    <resultMap id="AttributeResultMap" type="Attribute">
        <id column="id" property="id" jdbcType="VARCHAR"/>
        <result column="value" property="value" jdbcType="VARCHAR"/>
        <result column="feature_id" property="featureId" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id, value, feature_id
    </sql>

    <select id="findAll" resultMap="AttributeResultMap">
        SELECT <include refid="Base_Column_List"/> FROM attribute
    </select>
</mapper>