<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.productservice.mybatis.mapper.ProductFeatureMapper">
    <resultMap id="ProductFeatureResultMap" type="ProductFeature">
        <id column="product_id" property="productId" jdbcType="VARCHAR"/>
        <id column="feature_id" property="featureId" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        product_id, feature_id
    </sql>

    <select id="findAll" resultMap="ProductFeatureResultMap">
        SELECT <include refid="Base_Column_List"/> FROM product_feature
    </select>
</mapper>